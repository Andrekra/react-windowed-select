{
  "name": "react-windowed-select",
  "version": "1.0.2-beta",
  "description": "React Windowed Select",
  "main": "lib/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll"
  },
  "dependencies": {
    "prop-types": "15.6.2",
    "react-select": "3.0.4",
    "react-window": "1.8.2"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@storybook/addon-actions": "5.1.8",
    "@storybook/addon-links": "5.1.8",
    "@storybook/addons": "5.1.8",
    "@storybook/react": "5.1.8",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.4",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "chai": "4.2.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.14.0",
    "enzyme-to-json": "3.3.5",
    "jest": "23.6.0",
    "jest-dom": "3.0.0",
    "jest-in-case": "1.0.2",
    "nwb": "0.23.x",
    "ramda": "0.26.1",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-test-renderer": "16.8.6"
  },
  "jest": {
    "coveragePathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/tests/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 60,
        "functions": 90,
        "lines": 90,
        "statements": 90
      }
    },
    "setupTestFrameworkScriptFile": "<rootDir>/tests/setupTests.js",
    "transform": {
      "^.+\\.js$": "<rootDir>/jest.transform.js"
    }
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "react-component"
  ]
}
